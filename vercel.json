{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/debug.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/debug",
      "dest": "api/debug.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
