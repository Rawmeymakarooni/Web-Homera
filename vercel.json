{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "**/*.js",
          "**/*.json",
          "**/*.prisma",
          "prisma/**",
          "dao/**",
          "controller/**",
          "middleware/**",
          "routes/**",
          "services/**",
          "config/**",
          "node_modules/.prisma/**",
          "prisma/schema.prisma"
        ],
        "buildCommand": "npm run vercel-build"
      }
    },
    {
      "src": "api/debug.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/debug",
      "dest": "api/debug.js"
    },
    {
      "src": "/profil/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/prisma/profil/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/portofolio/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/prisma/portofolio/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/furnitur/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/prisma/furnitur/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/api",
      "dest": "api/index.js"
    },
    {
      "src": "/health",
      "dest": "api/index.js"
    },
    {
      "src": "/",
      "dest": "api/index.js"
    },
    {
      "src": "/favicon.ico",
      "dest": "api/index.js"
    },
    {
      "handle": "filesystem"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DISABLE_FS_OPERATIONS": "true",
